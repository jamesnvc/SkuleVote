<h1>New vote</h1>

<% form_for(@vote) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :election_id %><br />
    <%= f.text_field :election_id %>
  </p>
  <p>
    <%= f.label :choice_id %><br />
    <%= f.text_field :choice_id %>
  </p>
  <p>
    <%= f.label :ranking %><br />
    <%= f.text_field :ranking %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', votes_path %>